{
    "overrides": [
        {
            "files": [
                "*.js",
                "*.ts"
            ],
            "options": {
                "singleQuote": true,
                "semi": true,
                "trailingComma": "all",
                "bracketSpacing": true,
                "arrowParens": "always",
                "endOfLine": "lf"
            }
        }
    ]
}